{
  "name": "starter-fastify-js",
  "version": "0.0.0",
  "description": "Starter kit for server-side Node.js w/ Fastify implementations in Docker",
  "type": "module",
  "scripts": {
    "predev": "npm run tsc",
    "dev": "nodemon -w lib -x \"npm run start:dev\" & npm run tsc:watch & npm run test:watch",
    "fix": "npx eslint ./src --fix && npx prettier ./src --write",
    "lint": "npx eslint ./src && npx prettier ./src --check",
    "release": "cross-env NODE_ENV=production npm test && npm run tsc",
    "start": "NODE_ENV=production node ./bin/start.js",
    "start:dev": "NODE_ENV=development node ./bin/start.js",
    "pretest": "del coverage",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --env=node --colors --coverage 'src'",
    "test:watch": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --env=node --colors --coverage --watch 'src'",
    "pretsc": "del lib",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "version": "npm run release && git add --all",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fnalabs/starter-fastify-js.git"
  },
  "bugs": {
    "url": "https://github.com/fnalabs/starter-fastify-js/issues"
  },
  "homepage": "https://github.com/fnalabs/starter-fastify-js#readme",
  "author": "Adam Eilers",
  "license": "MIT",
  "engines": {
    "node": ">=24",
    "npm": ">=11"
  },
  "dependencies": {
    "@fastify/cors": "11.1.0",
    "@fastify/helmet": "13.0.2",
    "fastify": "5.6.1"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@tsconfig/node24": "24.0.1",
    "@types/jest": "30.0.0",
    "@types/json-schema": "7.0.15",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "cross-env": "10.1.0",
    "del-cli": "7.0.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.4.0",
    "jest": "30.2.0",
    "node-mocks-http": "1.17.2",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "ts-jest": "29.4.5",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1"
  },
  "volta": {
    "node": "24.9.0"
  }
}
